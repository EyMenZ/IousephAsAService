<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="330.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1"
fx:controller="com.iouseph.LoginLayoutController">
   <children>
      <VBox layoutX="84.0" layoutY="41.0" prefHeight="249.0" prefWidth="333.0">
         <children>
            <HBox prefHeight="81.0" prefWidth="333.0">
               <children>
                  <Label id="login_label" fx:id="loginLabel" text="Login">
                     <HBox.margin>
                        <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
                     </HBox.margin></Label>
                  <TextField id="login_textfield" fx:id="loginTextField">
                     <HBox.margin>
                        <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
                     </HBox.margin></TextField>
               </children>
            </HBox>
            <HBox prefHeight="86.0" prefWidth="333.0">
               <children>
                  <Label id="pwd_label" fx:id="pwdLabel" text="Password">
                     <HBox.margin>
                        <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
                     </HBox.margin></Label>
                  <PasswordField id="pwd_passwordfield" fx:id="pwdPasswrodField">
                     <HBox.margin>
                        <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
                     </HBox.margin>
                  </PasswordField>
               </children>
            </HBox>
            <HBox prefHeight="84.0" prefWidth="333.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#handleSignUp" prefWidth="100.0" text="Sign up">
                     <HBox.margin>
                        <Insets bottom="30.0" left="45.0" right="45.0" top="30.0" />
                     </HBox.margin></Button>
                  <Button mnemonicParsing="false" onAction="#handlelogin" prefWidth="100.0" text="Login">
                     <HBox.margin>
                        <Insets bottom="30.0" left="45.0" right="45.0" top="30.0" />
                     </HBox.margin></Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
